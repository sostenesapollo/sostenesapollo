    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@pps/eslint-config-pps": "^1.5.0",
    "@types/node": "^14.0.14",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.3.1",
